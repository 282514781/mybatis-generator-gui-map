<AnchorPane stylesheets="@../style.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zzg.mybatis.generator.controller.OverSshController">
  <children>
    <GridPane alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT" AnchorPane.topAnchor="15.0">
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="290.0" minWidth="19.0" prefWidth="194.0" />
